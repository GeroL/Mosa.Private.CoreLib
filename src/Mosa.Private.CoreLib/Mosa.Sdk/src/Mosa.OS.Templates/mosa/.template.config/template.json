{
  "$schema": "http://json.schemastore.org/template",
  "author": "MOSA",
  "classifications": [ "Mosa" ],
  "identity": "Mosa.App",
  "name": "Mosa .NET Application",
  "description": "A project for creating an Mosa .NET application",
  "shortName": "mosa",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "MosaApp1",
  "preferNameDirectory": true,
  "primaryOutputs": [
    { "path": "MosaApp1.csproj" }
  ],
  "symbols": {
    "packageName": {
      "type": "parameter",
      "description": "Overrides the package name in the mosa-manifest.xml",
      "datatype": "string",
      "replaces": "com.companyname.MosaApp1"
    }
  },
  "defaultName": "MosaApp1"
}